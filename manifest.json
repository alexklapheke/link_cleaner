{
    "author": "apiraino",
    "background": {
        "scripts": [
            "prefs.js",
            "cleaning_rules.js",
            "menu.js",
            "background.js",
            "copy.js"
        ]
    },
    "description": "Clean URLs before opening a link, removes trackers, rewrites redirects pages such as of Amazon, Facebook, Steam, Reddit (more to come!) and AMP urls.",
    "homepage_url": "https://github.com/apiraino/link_cleaner",
    "manifest_version": 2,
    "name": "Link Cleaner+",
    "applications": {
        "gecko": {
            "id": "{fc61c7db-fe1a-4bf8-a07e-a483a0ba2682}"
        }
    },
    "permissions": [
        "<all_urls>",
        "webRequest",
        "webRequestBlocking",
        "contextMenus",
        "menus",
        "storage"
    ],
    "options_ui": {
        "page": "options/options.html"
    },
    "version": "1.7",
    "default_locale": "en",
    "icons": {
        "48": "assets/icon.png",
        "96": "assets/icon@2x.png"
    }
}
